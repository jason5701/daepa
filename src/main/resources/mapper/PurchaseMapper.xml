<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.PurchaseMapper">
	<select id="order_List" resultType="com.example.domain.PurchaseVO">
		select * from orderlist where user_id=#{user_id} limit #{cri.pageStart}, #{cri.perPageNum}
	</select>
	<select id="admin_order_list" resultType="com.example.domain.PurchaseVO">
		select * from admin_order_List limit #{pageStart}, #{perPageNum}
	</select>
</mapper>